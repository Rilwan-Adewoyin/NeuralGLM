        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "justMyCode": false,
            
        },

        {
            "name": "Python: Gamma Hurdle",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            
            "args": [
                "--gpus","1",
                "--dataset","australia_rain",
                "--model_name","HLSTM",
                "--glm_name","DGLM",
                "--max_epochs", "150",

                "--target_distribution_name","gamma_hurdle",
                "--mean_distribution_name","normal",
                "--mean_link_name","relu",
                "--dispersion_distribution_name","gamma",
                "--dispersion_link_name","relu",
                "--pos_weight","1"
            ],
            
        },

